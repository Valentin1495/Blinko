{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "CONVEX_DEPLOYMENT": "dev:quick-pika-380",
        "EXPO_PUBLIC_CONVEX_URL": "https://quick-pika-380.convex.cloud"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "CONVEX_DEPLOYMENT": "prod:rightful-firefly-855",
        "EXPO_PUBLIC_CONVEX_URL": "https://rightful-firefly-855.convex.cloud"
      },
      "channel": "production"
    }
  }
}
